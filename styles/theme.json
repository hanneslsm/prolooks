		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "x-small",
					"shadow": "0 1px 2px 0 rgb(84 42 14/ 0.05)",
					"slug": "x-small"
				},
				{
					"name": "small",
					"shadow": "0 1px 3px 0 rgb(84 42 14/ 0.1), 0 1px 2px -1px rgb(84 42 14/ 0.1)",
					"slug": "small"
				},
				{
					"name": "medium",
					"shadow": "0 4px 6px -1px rgb(84 42 14/ 0.1), 0 2px 4px -2px rgb(84 42 14/ 0.1)",
					"slug": "medium"
				},
				{
					"name": "large",
					"shadow": "0 10px 15px -3px rgb(84 42 14/ 0.1), 0 4px 6px -4px rgb(84 42 14/ 0.1)",
					"slug": "large"
				},
				{
					"name": "x-large",
					"shadow": "0 20px 25px -5px rgb(84 42 14/ 0.1), 0 8px 10px -6px rgb(84 42 14/ 0.1)",
					"slug": "x-large"
				},
				{
					"name": "xx-large",
					"shadow": "0 25px 50px -12px rgb(84 42 14/ 0.25)",
					"slug": "xx-large"
